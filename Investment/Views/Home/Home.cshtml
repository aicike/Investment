@{
    ViewBag.Title = "首页";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- BEGIN DASHBOARD STATS -->
<div id="dashboard">
    <!-- BEGIN DASHBOARD STATS -->
    <div class="row-fluid">
        <div class="span3 responsive" data-tablet="span6" data-desktop="span3">
            <div class="dashboard-stat blue">
                <div class="visual">
                    <i class="icon-tasks"></i>
                </div>
                <div class="details">
                    <div class="number">
                        @ViewBag.DBCnt
                    </div>
                    <div class="desc">
                        待处理业务
                    </div>
                </div>
                <a class="more" href="@Url.Action("Backlog", "WorkFlow")">进入查看<i class="m-icon-swapright m-icon-white"></i>
                </a>
            </div>
        </div>
        <div class="span3 responsive" data-tablet="span6" data-desktop="span3">
            <div class="dashboard-stat green">
                <div class="visual">
                    <i class="icon-bell"></i>
                </div>
                <div class="details">
                    <div class="number">
                        @ViewBag.DDCnt</div>
                    <div class="desc">
                        待定业务</div>
                </div>
                <a class="more" href="@Url.Action("Pending", "WorkFlow")">进入查看 <i class="m-icon-swapright m-icon-white">
                </i></a>
            </div>
        </div>
        @if (ViewBag.ISDSH == "true")
        { 
            <div class="span3 responsive" data-tablet="span6  fix-offset" data-desktop="span3">
                <div class="dashboard-stat yellow">
                    <div class="visual">
                        <i class=" icon-user"></i>
                    </div>
                    <div class="details">
                        <div class="number">
                            @ViewBag.dshCnt</div>
                        <div class="desc">
                            待审核客户</div>
                    </div>
                    <a class="more" href="@Url.Action("IndexALL", "Financing")">进入查看 <i class="m-icon-swapright m-icon-white"></i></a>
                </div>
            </div>
        }
