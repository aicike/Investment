                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    7. 建设工程规划许可证</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "32", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.WuZheng_2 });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    8. 建筑工程施工许可证</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "33", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.WuZheng_3 });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    9. 商品房预售许可证</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "34", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.WuZheng_5 });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    10. 施工合同</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "25", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.ShiGongHeTong });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    11. 销售明细</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "26", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.XiaoShouMingXi });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    12. 过往房地产开发经验</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "28", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.GuoWangFangDiChanKaiFaJingYan });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
                                    13. 房管局档案查询</div>
                                <div style="float: left; margin-left: 50px">
                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "29", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = ViewBag.CompanyID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.FangGuanJuDangAnChaXun });}
                                </div>
                                <div style="clear: both">
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="myoption">
                    <div>
                        <button type="submit" class="btn blue submitbtn">
                            保存</button>
                        
                          <a href="@Url.Action("Index", "Company")" class="btn" style="">返回</a>
                    </div>
                </div>
            }
        </div>
    </div>
</div>