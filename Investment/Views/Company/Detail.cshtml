@{
    ViewBag.Title = "客户信息";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section scripts{
    <style>
        .myLink
        {
            cursor: pointer;
        }
        .form-horizontal .control-label
        {
            width: 115px;
        }
        .form-horizontal .controls
        {
            margin-left: 125px;
        }
    </style>
    <script>
        $(function () {
            var qiYeXingZhi = '@Model.QiYeXingZhi';
            $("#QiYeXingZhi option[value='" + qiYeXingZhi + "']").attr("selected", "selected");

            $("textarea").each(function (i, n) {
                autoTextarea(n, 15); // 调用
            });
        });
    </script>
}
@model Entity.Company
<div class="row-fluid">
    <div class="span12">
        <div class="tabbable tabbable-custom boxless">
            @using (Ajax.BeginForm("Edit", "Company", new AjaxOptions() { UpdateTargetId = "alert" }, new { @class = "form-horizontal" }))
            {
                <div class="myoption">
                    <div>
                        <button type="submit" class="btn blue submitbtn">
                            保存</button>
                        <button type="button" class="btn" onclick="location.href='@ViewBag.BackUrl'">
                            返回</button>@Html.HiddenFor(a => a.ID)@Html.HiddenFor(a => a.OwnerID)@Html.HiddenFor(a => a.Status)
                    </div>
                </div>
                <div class="portlet box blue">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-reorder"></i>公司基本资料</div>
                        <div class="tools">
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        公司名称</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.Name, new { @class = "span8 m-wrap", @readonly = "readonly" })
                                        <span class="help-inline">@Html.ValidationMessageFor(a => a.Name)</span>
                                    </div>
                                </div>
                            </div>
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        详细地址</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.Address, new { @class = "m-wrap span8", @readonly = "readonly" })
                                        <span class="help-inline">@Html.ValidationMessageFor(a => a.Address)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        公司网址</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.WebSite, new { @class = "m-wrap span8", @readonly = "readonly" })
                                        <span class="help-inline">@Html.ValidationMessageFor(a => a.WebSite)</span>
                                    </div>
                                </div>
                            </div>
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        公司电话</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.Phone, new { @class = "m-wrap span8", @readonly = "readonly" })
                                        <span class="help-inline">@Html.ValidationMessageFor(a => a.Phone)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row-fluid">
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        公司传真</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.Fax, new { @class = "m-wrap span8", @readonly = "readonly" })
                                        <span class="help-inline">@Html.ValidationMessageFor(a => a.Fax)</span>
                                    </div>
                                </div>
                            </div>
                            <div class="span6">
                                <div class="control-group">
                                    <label class="control-label">
                                        成立时间</label>
                                    <div class="controls">
                                        @Html.TextBox("SetUpTime", @Model.SetUpTime.ToString("yyyy/MM/dd"), new { @class = "m-wrap span6", placeholder = "年/月/日 （如：2014/05/01）", @readonly = "readonly" })
