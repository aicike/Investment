                                    <label class="control-label">
                                        开户许可证号</label>
                                    <div class="controls">
                                        @Html.TextBoxFor(a => a.KaiHuXuKeZheng, new { @class = "m-wrap span8", @readonly = "readonly" })
                                        <span class="help-inline">
                                            @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "5", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = Model.ID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.KaiHuXuKe_XinYongDaiMa, IsEnabled = false });}
                                        </span><span class="help-inline">@Html.ValidationMessageFor(a => a.KaiHuXuKeZheng)</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div class="row-fluid">
                            <div class="control-group">
                                <div style="margin-left: 59px; width: 550px; float: left;">
