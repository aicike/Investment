                                    @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "6", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = Model.ID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.YanZiBaoGao, IsEnabled = false });}
