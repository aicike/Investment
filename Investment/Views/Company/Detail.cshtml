                                            @{Html.RenderAction("Manage", "Attachment", new Investment.Models.AttachmentControl { ID = "1", Text = "[附件]", Table = Entity.SystemConst.TableName.Company, TableId = Model.ID, EnumAttachmentType = Entity.Enum.EnumAttachmentType.QiYeZiZhi,IsEnabled=false });}
                                        </span><span class="help-inline">@Html.ValidationMessageFor(a => a.QiYeZiZhi)</span>
                                    </div>
                                </div>
                            </div>
