@{
    ViewBag.Title = "修改客户";
    Layout = ViewBag.Layout;
    var CRList = ViewBag.CRList as List<Entity.CompanyReference>;
}
@section scripts{
    <style>
        .myLink
        {
            cursor: pointer;
        }
        .form-horizontal .control-label
        {
            width: 115px;
        }
        .form-horizontal .controls
        {
            margin-left: 125px;
        }
    </style>
    <script type="text/javascript">
        window.onbeforeunload = function () {
            var scrollPos;
            if (typeof window.pageYOffset != 'undefined') {
                scrollPos = window.pageYOffset;
            }
            else if (typeof document.compatMode != 'undefined' &&
          document.compatMode != 'BackCompat') {
                scrollPos = document.documentElement.scrollTop;
            }
            else if (typeof document.body != 'undefined') {
                scrollPos = document.body.scrollTop;
            }
            document.cookie = "scrollTop=" + scrollPos; //存储滚动条位置到cookies中  
        }

        window.onload = function () {
            if (document.cookie.match(/scrollTop=([^;]+)(;|$)/) != null) {
                var arr = document.cookie.match(/scrollTop=([^;]+)(;|$)/); //cookies中不为空，则读取滚动条位置  
                document.documentElement.scrollTop = parseInt(arr[1]);
                document.body.scrollTop = parseInt(arr[1]);
            }
        }




        var hasGuanLian = '@ViewBag.HasGuanLian';
        $(function () {
            var qiYeXingZhi = '@Model.QiYeXingZhi';
            $("#QiYeXingZhi option[value='" + qiYeXingZhi + "']").attr("selected", "selected");


            var url_gl = '@Url.Action("Add_GuanLian", "Company")';

            $("#link_open_guanlian").click(function () {
                $("#iframe_guanlian").attr("src", url_gl);
                init_gl();
            });
            init_gl();
        });
        function init_gl() {
            if (hasGuanLian == 'False') {
                $("#_Title").hide();
                $("#div_noMenu_top").hide();
                $("#div_noMenu_head").css("margin-top", "0px");
                $("#div_page_root").css("margin-top", "10px");
            }
        }
        function AddJGJKXX() {
            var url = '@Html.Raw(@Url.Action("JGJDXX", "Company", new { Types = 0, CompanyID = ViewBag.CompanyID }))';

            $("#iframe_JGJKXX").attr("src", url);
        }

        function EditJGJKXX(referenceid) {
            var url = '@Html.Raw(@Url.Action("JGJDXX", "Company", new { Types = 1, CompanyID = ViewBag.CompanyID }))';
            url = url + "&referenceID=" + referenceid
            $("#iframe_JGJKXX").attr("src", url);
        }

        function reloadJGJDXX() {
            location.reload();
        }
    </script>
}
